{
  "name": "recruit-wise",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run dev --prefix ./client",
    "admin": "npm run dev --prefix ./admin",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run admin\""
  },
  "keywords": [
    "recruitment",
    "candidate form",
    "labor contracts",
    "PDF uploads",
    "temp jobs"
  ],
  "author": "Tajinder singh",
  "license": "ISC",
  "description": "A web application for managing temporary labor contracts, allowing candidates to submit personal details and PDF documents.",
  "engines": {
    "node": "20.11.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.3",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.5",
    "sanitize-html": "^2.13.1",
    "slugify": "^1.6.6",
    "twilio": "^5.3.3"
  }
}
